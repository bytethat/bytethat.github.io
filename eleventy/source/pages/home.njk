<div class="region-area region-area-three">
    <div class="container-xxl">
        <div class="inner-area inner-area-one">
    
            <h2>Our work</h2>

            <div class="projects">
                {% for project in projects %}
                <a href="{{ project.url }}" class="card project">
                    <figure class="card-image">
                        <img src="{{ project.image }}" alt="{{ project.title }}" />
                    </figure>
                    <div class="card-body">
                        <h3 class="card-title">{{ project.title }}</h3>
                        <p>{{ project.description }}</p>
                    </div>
                </a>
                {% endfor %}
            </div>
        </div>

    </div>
</div>

<div class="region-area region-area-four">
    <div class="container-xxl">
        <div class="inner-area inner-area-one">
            <h2>What our clients say</h2>

            <div class="testimonials">

                {% for testimonial in testimonials %}
                <div class="card testimonial">
                    <figure class="card-image">
                        <img src="{{ testimonial.image }}" alt="{{ testimonial.author.company }}" />
                    </figure>
                    <figure class="card-body">
                        <blockquote class="quote">
                            {{ testimonial.quote }}
                        </blockquote>
                        <figcaption class="author">
                            <span class="name">{{ testimonial.author.name }}</span>
                            <span class="role">{{ testimonial.author.role }}</span>
                            <span class="company">{{ testimonial.author.company }}</span>
                        </figcaption>
                    </figure>
                </div>
                {% endfor %}
            </div>
        </div>
    </div>
</div>

